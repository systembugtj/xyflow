{
  "name": "@xyflow/vanilla",
  "version": "0.1.0",
  "description": "Vanilla Flow - A framework-agnostic library for building node-based editors and interactive flow charts.",
  "keywords": [
    "vanilla",
    "node-based UI",
    "graph",
    "diagram",
    "workflow",
    "xyflow",
    "vanilla-flow"
  ],
  "files": [
    "dist",
    "src",
    "!**/__tests__",
    "!**/test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xyflow/xyflow.git",
    "directory": "packages/vanilla"
  },
  "homepage": "https://xyflow.com",
  "bugs": {
    "url": "https://github.com/xyflow/xyflow/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/base.css": "./dist/base.css",
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && pnpm css",
    "css": "postcss src/styles/{base,style}.css --config ./../../tooling/postcss-config/ --dir dist",
    "css-watch": "pnpm css --watch",
    "lint": "eslint --ext .js,.ts,.tsx,.wsx src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@xyflow/system": "workspace:*",
    "@wsxjs/wsx-core": "^0.0.14"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@xyflow/eslint-config": "workspace:*",
    "@xyflow/rollup-config": "workspace:*",
    "@xyflow/tsconfig": "workspace:*",
    "@wsxjs/wsx-vite-plugin": "^0.0.14",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^11.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "postcss-rename": "^0.6.1",
    "typescript": "5.4.5",
    "vite": "^5.0.0"
  }
}
